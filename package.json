{
  "name": "nextjs-web3",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "dev": "next",
    "docker": "chmod +x docker.sh && ./docker.sh",
    "watch": "tsc -w -p ./tsconfig.json",
    "build": "next build",
    "start": "next start -p 8080",
    "pre-commit": "lint-staged",
    "type-check": "tsc -p ./tsconfig.json --pretty --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "release": "standard-version"
  },
  "lint-staged": {
    ".": "npm run type-check",
    ".**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run prettier:fix"
    ]
  },
  "dependencies": {
    "@dynamic-labs/ethereum": "^1.0.7",
    "@dynamic-labs/magic": "^1.0.7",
    "@dynamic-labs/sdk-react-core": "^1.0.7",
    "@dynamic-labs/wagmi-connector": "^1.0.7",
    "@emotion/react": "^11.11.1",
    "@inverter-network/abis": "^0.0.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.13.4",
    "classnames": "^2.3.2",
    "next": "latest",
    "next-nprogress-bar": "^2.1.2",
    "react": "18.2.0",
    "react-daisyui": "5.0.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.14",
    "daisyui": "^4.4.17",
    "eslint": "^8.55.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.1.6",
    "viem": "^1.19.13",
    "wagmi": "^1.4.12"
  },
  "browser": {
    "fs": false,
    "path": false
  }
}
